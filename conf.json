{
    "ssh": {
        "timeout": 5,
        "max_retries": 1,
        "auth_timeout": 5,
        "banner_timeout": 5,
        "allow_agent": false,
        "look_for_keys": false,
        "success_file": "success.txt"
    },
    "async": {
        "max_workers": 50
    },
    "logging": {
        "log_level": "DEBUG",
        "log_file": "app.log",
        "log_format": "%(asctime)s - %(levelname)s - %(message)s",
        "log_datefmt": "%Y-%m-%d %H:%M:%S"
    }
}